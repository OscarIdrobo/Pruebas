<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre usuario</th>
      <th>Apellido usuario</th>
      <th>Tipo documento</th>
      <th>Documento</th>
      <th>Cel personal</th>
      <th>Correo personal</th>
      <th>Contrasena</th>
      <th>Direccion residencia</th>
      <th>Latitud dir residencia</th>
      <th>Longitud dir encargado</th>
      <th>Fecha creacion</th>
      <th>Estado</th>
      <th>Rol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.Nombre_usuario %></td>
        <td><%= usuario.Apellido_usuario %></td>
        <td><%= usuario.Tipo_documento %></td>
        <td><%= usuario.Documento %></td>
        <td><%= usuario.Cel_personal %></td>
        <td><%= usuario.Correo_personal %></td>
        <td><%= usuario.Contrasena %></td>
        <td><%= usuario.Direccion_residencia %></td>
        <td><%= usuario.Latitud_dir_residencia %></td>
        <td><%= usuario.Longitud_dir_encargado %></td>
        <td><%= usuario.Fecha_creacion %></td>
        <td><%= usuario.Estado %></td>
        <td><%= usuario.Rol %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
