<p id="notice"><%= notice %></p>

<h1>Tipo Donaciones</h1>

<table>
  <thead>
    <tr>
      <th>Desc tipo donacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_donaciones.each do |tipo_donacione| %>
      <tr>
        <td><%= tipo_donacione.Desc_Tipo_Donacion %></td>
        <td><%= link_to 'Show', tipo_donacione %></td>
        <td><%= link_to 'Edit', edit_tipo_donacione_path(tipo_donacione) %></td>
        <td><%= link_to 'Destroy', tipo_donacione, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Donacione', new_tipo_donacione_path %>
